{
    "name": "easir/error-normalizer-bundle",
    "description": "Unified ConstraintViolationList Normalizer",
    "type": "symfony-bundle",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/easir/coding-standard.git"
        }
    ],
    "require": {
        "php": "^7.2|~8.0.0",
        "symfony/serializer": "^4.4 || ^5.0",
        "symfony/validator": "^4.4 || ^5.0",
        "symfony/framework-bundle": "^4.4 || ^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "easir/coding-standard": "1.0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Micha≈Ç Brzuchalski",
            "email": "michal.brzuchalski@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Easir\\ErrorNormalizerBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Easir\\ErrorNormalizerBundle\\Tests\\": "tests/"
        }
    }
}
